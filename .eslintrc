{
  "root": true,
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:vue/vue3-recommended", "eslint:recommended"
  ],
  "plugins": [
    "vue"
  ],
  "parser": "vue-eslint-parser",
  "rules": {
    "no-case-declarations": "off",
    "no-use-before-define": "off",
    "no-undef": "off",
    "vue/comment-directive": "off"
  }
}
